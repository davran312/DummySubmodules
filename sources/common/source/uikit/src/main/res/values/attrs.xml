<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="colorSecondaryDark" format="color|reference" />
    <attr name="colorSecondaryLight" format="color|reference" />
    <attr name="colorHighEmphasisOnSecondary" format="color|reference" />
    <attr name="colorMediumEmphasisOnSecondary" format="color|reference" />
    <attr name="colorDisabledOnSecondary" format="color|reference" />
    <attr name="colorHighEmphasisOnPrimary" format="color|reference" />
    <attr name="colorMediumEmphasisOnPrimary" format="color|reference" />
    <attr name="colorFootnotesOnPrimary" format="color|reference" />
    <attr name="colorDisabledOnPrimary" format="color|reference" />
    <attr name="colorErrorOnPrimary" format="color|reference" />
    <attr name="colorAlertBackground" format="color|reference" />
    <attr name="colorMessage" format="color|reference" />
    <attr name="colorApply" format="color|reference" />

    <declare-styleable name="CounterFloatingActionButton">
        <attr name="textColor" format="color|reference" />
        <attr name="textBackground" format="integer|reference" />
    </declare-styleable>

    <attr name="text" format="string" />
    <attr name="enabled" format="boolean" />

    <attr name="placeholderDrawable" format="reference" />
    <attr name="placeholderTint" format="reference" />

    <declare-styleable name="ImageWithPlaceholderView">
        <attr name="placeholderDrawable" />
        <attr name="placeholderTint" />
        <attr name="clipImageView" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RoundedFrameLayout">
        <attr name="android:radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="EndlessScrollRecyclerView">
        <attr name="emptyLayout" format="reference" />
        <attr name="errorLayout" format="reference" />
        <attr name="progressLayout" format="reference" />
        <attr name="android:nestedScrollingEnabled" />
    </declare-styleable>

    <declare-styleable name="ProgressButton">
        <attr name="android:textAppearance" format="reference" />
        <attr name="buttonText" format="string" />
        <attr name="android:textColor"/>
        <attr name="progressColor" format="color" />
        <attr name="buttonTextAllCaps" format="boolean" />
        <attr name="buttonImage" format="reference" />
        <attr name="android:textStyle" />
        <attr name="enabled" />
    </declare-styleable>

    <declare-styleable name="FullScreenErrorView">
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="SimpleErrorView">
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="NumberKeyboardButtonView">
        <attr name="keyboard_button_text" format="string" />
        <attr name="keyboard_button_image" format="integer" />
    </declare-styleable>

    <declare-styleable name="NumberKeyboardView">
        <attr name="pressed_button_color" format="color" />
    </declare-styleable>

    <declare-styleable name="TitledTextView">
        <attr name="text" />
        <attr name="title" />
        <attr name="titlePosition" format="enum">
            <enum name="bottom" value="0" />
            <enum name="top" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="NambaToolbar">
        <attr name="navigationTint" format="reference" />
        <attr name="toolbarBackground" format="reference" />
        <attr name="elevated" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ProfileSettingsRowTextView">
        <attr name="topText" format="string" />
        <attr name="bottomText" format="string" />
        <attr name="icon" format="reference" />
        <attr name="iconTint" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="ExpandableLayout">
        <attr name="duration" format="integer" />
        <attr name="expanded" format="boolean" />
        <attr name="parallax" format="float" />
        <attr name="android:orientation" />
    </declare-styleable>

    <declare-styleable name="NambaCheckBox">
        <attr name="indeterminate" format="boolean" />
        <attr name="checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ExpandHeaderView">
        <attr name="title" format="string" />
    </declare-styleable>

    <declare-styleable name="NambaTextField">
        <attr name="labelColor" format="color" />
        <attr name="android:textColor"/>
        <attr name="focusedBorderColor" format="color" />
        <attr name="borderColor" format="color" />
        <attr name="editable" format="boolean" />
        <attr name="isDropdown" format="boolean" />
        <attr name="labelText" format="string" />
        <attr name="android:digits"/>
        <attr name="hintText" format="string" />
        <attr name="android:inputType" />
        <attr name="android:maxLength"/>
        <attr name="android:imeOptions" />
        <attr name="android:text" />
        <attr name="android:hint"/>
    </declare-styleable>

    <declare-styleable name="ColorButton">
        <attr name="android:text" format="string"/>
        <attr name="android:description" format="string"/>
    </declare-styleable>
</resources>